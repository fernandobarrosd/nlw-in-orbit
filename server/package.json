{
  "name": "in-orbit",
  "version": "1.0.0",
  "main": "./dist/http/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch --env-file .env src/http/server.ts",
    "create:migrations": "yarn drizzle-kit generate",
    "run:migrations": "yarn drizzle-kit migrate",
    "drizzle-studio": "yarn drizzle-kit studio",
    "run:seed": "tsx --env-file .env src/db/seed.ts",
    "build": "tsc",
    "prod": "yarn run:migrations && yarn run:seed && yarn build"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "drizzle-kit": "^0.24.2",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.33.0",
    "fastify": "^4.28.1",
    "fastify-type-provider-zod": "^2.0.0",
    "postgres": "^3.4.4",
    "zod": "^3.23.8"
  }
}
